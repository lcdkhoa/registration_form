{
  "arrowParens": "always",
  "bracketSpacing": true,
  "printWidth": 80,
  "useTabs": true,
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "trailingComma": "es5",
  "proseWrap": "preserve",
  "importOrder": [
    "<THIRD_PARTY_TS_TYPES>",
    "<TS_TYPES>^[./]",
    "<THIRD_PARTY_MODULES>",
    "^[./]"
  ],
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}